{
  "expo": {
    "name": "KuluCash",
    "slug": "kulu-cash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kulu-cash",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ericson.kulucash"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1A1C1E"
      },
      "package": "com.ericson.kulucash"
    },
    "plugins": [
      "expo-router",
      [
        "@morrowdigital/watermelondb-expo-plugin"
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
